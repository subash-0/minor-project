{
  "compilerOptions": {
    "jsx": "react-jsx", // Ensure this is present
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "moduleResolution": "node",
    "module": "ESNext",
    "target": "ESNext",
    "lib": ["dom", "esnext"],
    "allowJs": true
  },
  "include": [
    "src/**/*"
  ]
}
